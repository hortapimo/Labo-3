import pandas as pd
import numpy as np
import matplotlib.pyplot as plt

df=pd.read_csv("datos/datos_RC.csv")

frec= df["Freq"]
fase = df["fase_out"]


# %%
fig, ax = plt.subplots(figsize=(10,4))
ax.set_xscale('log')
ax.set_xlabel("Frecuencia [Hz]")
ax.set_ylabel("Fase [Â°]")
ax.minorticks_on()
ax.grid(which='minor',alpha=0.3)
ax.grid(which='major',alpha=1)
ax.scatter(frec,fase,c='black',alpha=0.7,zorder=10)

plt.tight_layout()
#%%